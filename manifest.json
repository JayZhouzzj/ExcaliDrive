{
    "manifest_version": 3,
    "name": "Excalidraw Google Drive Extension",
    "version": "1.0",
    "permissions": ["storage", "identity", "https://www.googleapis.com/"],
    "background": {
        "service_worker": "dist/background.js"
    },
    "action": {
        "default_popup": "popup.html"
    }
}
